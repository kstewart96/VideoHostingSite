            <div>
                

                <div">
                    @RenderBody()
                </div">

                <hr />
                <footer>
                    <div>
                        &copy; 2019 Video Hosting All rights reserved. All trademarks are property of their respective owners in the US and other countries.
                        VAT included in all prices where applicable.

                    </div>
                </footer>
            </div>




        </div>*@
</body>
</html>

